{
  "manifest_version": 2,
  "name": "Flow",
  "description": "Flow is a Chrome Extension by Cometic Digital to create a New Tab menu.",
  "version": "1.00",
  "browser_action": {
    "default_icon" : "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }

  ]
}